    <form class="form">
      <div class="primeira-linha-form">
        <div class="emissor">
          <label for="">Colaborador:</label>
          <select id="selectTec" onchange="mostrarDados('<%= JSON.stringify(horas) %>')">
            <% Tecnicos.forEach(function(tec) { %>
              <option value="<%=tec.matricula_tec %>"><%=tec.nome_tec  %></option>
          <% }); %>
            
           
          </select>
        </div>
        <div class="tempo-quantidade">
          <div class="tempo">
            <label for="">Tempo</label>
            <input type="time" required />
          </div>

          <div class="quantidade">
            <label for="">Corretiva:</label>
            <input type="number" required />
          </div>
        </div>
      </div>
      <div class="segunda-linha-form">
        <div>
          <label for="">Emissor: </label>
          <select id="Emissor">
            <option value="lucas">Eduardo Wagner Gregorio</option>
            <option value="lucas">Jean victor dos Santos Maciel</option>
            <option value="eduardo">Lucas Silva Cuba</option>
            <option value="victor">Lucas Ulbrich Franco</option>
            <option value="jean">Luiz Gabriel Roberti</option>
            <option value="luiz">Victor Bueno Cardoso</option>
          </select>
        </div>

        <div class="tempo-quantidade">
          <div class="tempo">
            <label for="">Tempo</label>
            <input type="time" required />
          </div>

          <div class="quantidade">
            <label for="">Preventiva</label>

            <input type="number" required />
          </div>
        </div>
      </div>
      <div class="terceira-linha-form">
        <div class="periodo">
          <label for="">Periodo:</label>
          <div class="datas">
            <input class="hora" type="date" required />
            <input class="hora" type="date" required />
          </div>
        </div>
      </div>
      <footer class="footer-bg">
        <div class="footer">
          <label for="">Total Geral</label>
          <div class="total-tempo">
            <p>17:53</p>
          </div>
          <di class="total-quantidade">
            <p>11</p>
          </di>
        </div>
      </footer>
      <button type="submit" class="submit-button">Enviar</button>
    </form>
